{
  "name": "Google Calendar",
  "version": "1.0.17",
  "slug": "gcalendar",
  "description": "Zobrazí Google Kalendář jako dlaždici v Home Assistantu.",
  "arch": [
    "amd64",
    "aarch64",
    "armv7"
  ],
  "startup": "services",
  "boot": "auto",
  "options": {
    "certfile": "/data/cert.pem",
    "keyfile": "/data/privkey.pem"
  },
  "schema": {
    "certfile": "str",
    "keyfile": "str"
  },
  "ports": {
    "8000/tcp": 8000
  },
  "map": [
    "data:rw"
  ],
  "webui": "https://[HOST]:[PORT:8000]",
  "host_network": true
}